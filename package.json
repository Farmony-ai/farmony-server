{
  "name": "rural-share-backend",
  "version": "1.0.0",
  "description": "API codebase",
  "main": "src/main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=prod node dist/main.js",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch ",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "build": "nest build",
    "start:dev:old": "cross-env NODE_ENV=dev ts-node-dev --respawn --transpile-only src/main.ts",
    "test": "cross-env NODE_ENV=test jest",
    "test:unit:matches": "cross-env NODE_ENV=test jest -i src/modules/matches/matches.service.spec.ts",
    "test:e2e:matches": "cross-env NODE_ENV=test jest -i src/modules/matches/matches.http.e2e.spec.ts",
    "cleanup:listings": "ts-node src/scripts/cleanup-invalid-listings.ts",
    "migrate:addresses": "ts-node src/scripts/migrate-addresses.ts"
  },
  "author": "Anirudh Saladi",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-socket.io": "^11.1.4",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.0.1",
    "mongoose": "^8.16.2",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "jest": "^30.0.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
